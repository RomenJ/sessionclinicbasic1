{% extends 'base.html.twig' %}

{% block title %}Sessionvaloracion index{% endblock %}

{% block body %}
    <h1>Sessionvaloracion index</h1>

    <table class="table">
        <thead>
            <tr>
                     <th>Paciente</th>
                     <th>Médico</th>
                     <th>Patología diagnosticada 1</th>
                     <th>Patología diagnosticada 2</th>
                     <th>Patología diagnosticada 3</th>
                <th>Fecha</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sessionvaloracion in sessionvaloracions %}
            <tr>
     
                <td>{{ sessionvaloracion.paciente }}</td>
                <td>{{ sessionvaloracion.medico }}</td>
                <td>{{ sessionvaloracion.patologia1 }}</td>
                <td>{{ sessionvaloracion.patologia2 }}</td>
                <td>{{ sessionvaloracion.patologia3 }}</td>
                <td>{{ sessionvaloracion.fecha ? sessionvaloracion.fecha|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_sessionvaloracion_show', {'id': sessionvaloracion.id}) }}">show</a>
                    <a href="{{ path('app_sessionvaloracion_edit', {'id': sessionvaloracion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_sessionvaloracion_new') }}">Create new</a>
{% endblock %}
